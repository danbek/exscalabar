(function() {
	angular.module('main').controller('InputTable', ['$scope', 'tableService',
	function($scope, tableService) {

		$scope.data = [];

		$scope.$on('handleBroadcast', function() {

			var tID = tableService.getTab();
			var val = "";
			switch (tID) {
			case "O3 Valve":
			case"O2 Valve":
			case"O3 Generator":
			case "Filter":
				val = 'FALSE';
				break;
			case "Wait":
			case "Speaker":
				val = "20";
				break;

			case "QO2":
				val = "100";
				break;
			default:
			}
			$scope.data.push({
				"id" : tID,
				"val" :val
			});
		});

	}]);
})();
