(function(){
	angular.module('main')
	.factory('tableService', function(){
		var tabService = {
			curTab: '',
			
		}
		
		tabService.curTab = '';
		tabService.setTab = function(tab){
			this.curTab = tab;
			};
			
		tabService.getTab = function(){
			return this.curTab;
			};
		
		return tabService;
	});
	
})();
